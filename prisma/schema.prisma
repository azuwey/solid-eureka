// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./solid-eureka.db"
}

model Nationality {
  id                Int      @id @default(autoincrement())
  abbreviation      String   @unique
  voteCount         Int
}
